import java.util.ArrayList;
import java.util.List;

public class Carte 
{
	CouleurEnum couleur;
	ValeurEnum valeur;
	
	public Carte(CouleurEnum c, ValeurEnum v)
	{
		this.couleur = c;
		this.valeur = v;
	}
	
	public static List paquetComplet()
	{
		ArrayList paquet = new ArrayList();
		paquet.add(arg0)
	}
}
